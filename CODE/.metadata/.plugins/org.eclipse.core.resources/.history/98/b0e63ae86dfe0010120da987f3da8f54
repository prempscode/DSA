package DSA;

public class Solution744 {
    public char nextGreatestLetter(char[] letters, char target) {
    	char min = Character.MIN_VALUE;
        for(char ch : letters) {
        	if(ch > target && min > ch)
        		min = ch;
        }
        return min;
    }
    public static void main (String[] args) {
    	System.out.println((int)Character.MIN_VALUE);
    }
}
