package DSA;

public class Solution1 {
    public static int[] twoSum(int[] nums, int target) {
		int res[] = {-1,-1};
		int i = 0;
		int j = nums.length-1;
		int newnums[] = nums;
		 java.util.Arrays.sort(newnums);
		while(i<=j) {
			if((newnums[i] + newnums[j]) == target) {
				break;
			}
			else if((newnums[i] + newnums[j]) > target) {
				j--;
			}
			else {
				i++;
			}
		} 	
		res[0] = findIdx(nums,nums[i]);
		res[1] = findIdx(nums,nums[j]);
		
    	return res;
    }
    private static int findIdx(int[] nums, int i) {
		
		return 0;
	}
	public static void main(String[] args) {
    	int nums[] = {3,2,4};
    	int target = 6;
    	int res[] = twoSum(nums , target);
    	for(int i : res) {
    		System.out.print(i+" ");
    	}
    }
}