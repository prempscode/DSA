package DSA;

public class Solution1 {
    public static int[] twoSum(int[] nums, int target) {
		int res[] = {0,0};
		int i = 0;
		int j = nums.length-1;
		
		while(i<=j) {
			if((nums[i] + nums[j]) == target) {
				res[0] = i;
				res[1] = j;
				return res;
			}
				
		}
    	
    	
    	
    	return nums;
        
    }
    public static void main(String[] args) {
    	
    }
}