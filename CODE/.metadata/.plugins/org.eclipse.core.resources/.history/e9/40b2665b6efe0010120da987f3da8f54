package DSA;

public class Solution744 {
    public static char nextGreatestLetter(char[] letters, char target) {
    	char min = Character.MAX_VALUE;
        for(char ch : letters) {
        	if(ch > target && min > ch)
        		min = ch;
        }
        return min;
    }
    public static void main (String[] args) {
    	int char[] = [];
    	char result = nextGreatestLetter(['c','f','j'] , 'a');
    }
}
